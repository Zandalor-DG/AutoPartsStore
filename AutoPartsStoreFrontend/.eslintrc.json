{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["react-app", "prettier", "plugin:prettier/recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "semi": false,
        "jsxSingleQuote": true
      }
    ]
  }
}
